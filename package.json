{
	"name": "dome-app",
	"productName": "Dome App",
	"version": "1.0.1",
	"description": "The best app ever",
	"license": "MIT",
	"repository": "javaarchive/DomeApp",
	"author": {
		"name": "Raymond",
		"email": "awesomecatstudio@gmail.com",
		"url": "https://ascent.tk"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --linux --windows",
		"wrap": "parcel build -d static/dist --target electron -o bundle.js static/unbundle.js",
		"wrap-watch": "parcel watch -d static/dist --target electron -o bundle.js static/unbundle.js",
		"run-server": "cd mediaserver && node server.js",
		"quick-api-test": "cd mediaserver && node api_sequelize.js",
		"temp": "pwd"
	},
	"dependencies": {
		"@babel/runtime": "^7.12.5",
		"@cliqz/adblocker-electron": "^1.19.0",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"ascii-table": "0.0.9",
		"body-parser": "^1.19.0",
		"commander": "^6.2.0",
		"dotenv": "^8.2.0",
		"electron-context-menu": "^0.15.2",
		"electron-debug": "^3.1.0",
		"electron-store": "^5.2.0",
		"electron-unhandled": "^3.0.2",
		"electron-updater": "^4.3.5",
		"electron-util": "^0.13.1",
		"express": "^4.17.1",
		"express-session": "^1.17.1",
		"fontsource-roboto": "^3.1.5",
		"i18n": "^0.10.0",
		"id-auto-increment": "^1.0.1",
		"jquery": "^3.5.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.20",
		"new-github-issue-url": "^0.2.1",
		"node-forge": "^0.10.0",
		"node-pre-gyp": "^0.16.0",
		"parcel-bundler": "^1.12.4",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"regenerator-runtime": "^0.13.7",
		"sequelize": "^6.3.5",
		"sqlite3": "^5.0.0",
		"uid-generator": "^2.0.0",
		"uri": "^0.1.0",
		"url-regex": "^5.0.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.12.1",
		"autoprefixer": "^9.8.6",
		"electron": "^7.3.3",
		"electron-builder": "^21.2.0",
		"np": "^5.2.1",
		"postcss": "^8.2.1",
		"postcss-modules": "^3.2.2",
		"xo": "^0.33.1"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "io.github.the-ascent",
		"mac": {
			"category": "public.app-category.music",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Music;Video"
		}
	}
}
